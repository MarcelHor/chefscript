{
  "name": "chefscript",
  "displayName": "ChefScript",
  "description": "ChefScript Language Server",
  "author": "Marcel Horv√°th",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.1",
    "@vscode/test-electron": "^2.3.9"
  },
  "contributes": {
    "languages": [
      {
        "id": "chefscript",
        "aliases": ["ChefScript", "chefscript"],
        "extensions": [".chef"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "chefscript",
        "scopeName": "source.chefscript",
        "path": "./syntaxes/chefscript.tmLanguage.json"
      }
    ]
  }
}
